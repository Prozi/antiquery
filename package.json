{
  "name": "antiquery",
  "version": "1.3.0",
  "description": "a *very* slim package for basic DOM operations - few bytes",
  "main": "index.js",
  "scripts": {
    "test": "killall node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Prozi/antiquery.git"
  },
  "keywords": [
    "query",
    "querySelector",
    "querySelectorAll"
  ],
  "author": "Jacek Pietal (prozi85@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "editions": "^1.3.3"
  },
  "editions": [
    {
      "description": "Source + ESNext + Import",
      "entry": "source/index.js",
      "syntaxes": ["javascript", "esnext", "import", "arrows", "const", "let", "destructuring"]
    },
    {
      "description": "Babel Compiled + ES2015 + Require",
      "entry": "es2015/index.js",
      "syntaxes": ["javascript", "es2015", "require"]
    }
  ],
  "scripts": {
    "compile": "babel ./source --out-dir ./es2015 --presets es2015"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0"
  }
}
